{
  "name": "nestjs-server",
  "version": "1.0.0",
  "description": "NestJs application server",
  "author": "",
  "license": "ISC",
  "main": "dist/main.js",
  "homepage": "https://github.com/AndresPrez/nestjs-server-template#readme",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "yarn run copy-assets",
    "copy-assets": "copyfiles -f src/public/* dist/public",
    "start": "nest start",
    "start:dev": "concurrently \"nest start --watch\" \"yarn copy-assets\" ",
    "start:debug": "nest start --debug --watch",
    "dc:dev": "docker-compose up dev db",
    "start:prod": "node dist/main",
    "refresh-schema": "yarn typeorm schema:drop -c seed && yarn typeorm schema:sync -c seed",
    "seed-data": "yarn typeorm migration:run -c seed",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndresPrez/nestjs-server-template.git"
  },
  "bugs": {
    "url": "https://github.com/AndresPrez/nestjs-server-template/issues"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.4",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/serve-static": "^2.1.4",
    "@nestjs/swagger": "^5.0.4",
    "@nestjs/typeorm": "^8.0.1",
    "basic-auth": "^2.0.1",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@babel/runtime": "^7.14.6",
    "@nestjs/cli": "^8.0.2",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@types/basic-auth": "^1.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.22",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.36",
    "@types/passport-http": "^0.3.8",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "concurrently": "^6.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.28.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.4"
  },
  "_moduleAliases": {
    "src": "dist",
    "@configs": "dist/configs/",
    "@state": "dist/state/",
    "@modules": "dist/modules/",
    "@entities": "dist/orm/entities/"
  },
  "engineStrict": true,
  "engines": {
    "node": ">=12.16"
  }
}
